#FUNK
function gacp() {
    git add .
    git commit -a -m "$1"
    git push
}

function gac() {
    git add .
    git commit -a -m "$1"
}

function gcb() {
    git fetch
    git pull
    git checkout -b "$1"
    git push -u origin "$1"
}

function gc() {
    git checkout "$1"
}

# git delete remote branch
function gdrb () {
    git push origin --delete "$1"
}

# git delete local branch
function gdlb () {
    git branch -D "$1"
}

